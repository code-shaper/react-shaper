    "cypress": "^12.11.0",
    "eslint": "^8.39.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.0",
    "eslint-plugin-storybook": "^0.6.12",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "lint-staged": "^13.2.1",
    "msw": "1.2.1",
    "prettier": "^2.8.8",
    "prop-types": "^15.8.1",
    "storybook": "^7.0.22",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.4",
    "vite": "^4.3.3",
    "vite-tsconfig-paths": "^4.2.0"
