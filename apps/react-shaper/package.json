{
  "name": "react-shaper",
  "description": "React Shaper",
  "version": "1.0.2",
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/index.js",
  "files": [
    "bin/react-shaper",
    "dist",
    "README.md"
  ],
  "bin": {
    "react-shaper": "bin/react-shaper"
  },
  "scripts": {
    "build": "rimraf dist && tsc && copyfiles --all --up 1 \"src/**/templates/**/*\" dist",
    "lint": "eslint src/**/*.ts*",
    "test": "echo No tests in this package yet",
    "clean": "rimraf .turbo node_modules dist coverage **/test-output"
  },
  "dependencies": {
    "@code-shaper/react-starter": "*",
    "@code-shaper/shaper-utils": "*",
    "inquirer": "^8.2.5",
    "inquirer-directory": "^2.2.0",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.2",
    "@types/inquirer": "^8.2.6",
    "@types/node": "^20.3.1",
    "@types/yargs": "^17.0.24",
    "copyfiles": "^2.4.1",
    "eslint": "^8.43.0",
    "eslint-config-custom": "*",
    "jest": "^29.5.0",
    "jest-config-custom": "*",
    "ts-jest": "^29.1.0",
    "typescript": "^5.1.3",
    "typescript-config-custom": "*"
  }
}
